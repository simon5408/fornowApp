<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="main_tab_but_linear">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="main_menu_icon">
        <item name="android:layout_marginTop">7dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_gravity">center_horizontal|center_vertical</item>
        <item name="android:layout_weight">3</item>
    </style>

    <style name="main_menu_text">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">#727272</item>
        <item name="android:layout_weight">4</item>
    </style>

    <style name="header_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">55dip</item>
        <item name="android:background">@drawable/header_bg</item>
    </style>

    <style name="home_introduce">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dip</item>
        <item name="android:background">@drawable/indroduce_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/home_introColor</item>
        <item name="android:textSize">14sp</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="home_pullToRefresh_style">
        <item name="android:layout_marginTop">55dip</item>
        <item name="android:background">#ffffff</item>
    </style>

    <style name="shopcart_bg">
        <item name="android:layout_marginTop">55dip</item>
        <item name="android:background">@color/gray_bg_color</item>
    </style>

    <style name="mine_content_style">
        <item name="android:layout_marginTop">55dip</item>
        <item name="android:background">#ffffff</item>
    </style>

    <style name="item01_img">
        <item name="android:layout_height">125dip</item>
    </style>

    <style name="home_banner_style">
        <item name="android:layout_height">180dip</item>
    </style>

    <style name="home_padding">
        <item name="android:layout_marginTop">8dip</item>
        <item name="android:paddingLeft">8dip</item>
        <item name="android:paddingTop">8dip</item>
        <item name="android:paddingRight">8dip</item>
    </style>

    <style name="home_tuijian_text">
        <item name="android:textSize">20sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/manColor</item>
    </style>

    <style name="goods_name_text">
        <item name="android:textSize">16sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/goodsname</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="green_text">
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/green</item>
    </style>

    <style name="goods_originPrice_text_style">
        <item name="android:textSize">14sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/originPriceColor</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="cart_introduce_text_style">
        <item name="android:textSize">14sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/manColor</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="goods_unit_text">
        <item name="android:textSize">16sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/currentPriceColor</item>
    </style>

    <style name="total_price_lab">
        <item name="android:textSize">16sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/goodsname</item>
    </style>

    <style name="header_logo_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:src">@drawable/logo</item>
    </style>

    <style name="header_text_style">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="home_corner_mark">
        <item name="android:layout_marginRight">10dip</item>
        <item name="android:layout_marginTop">4dip</item>
        <item name="android:layout_height">16dip</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="home_block_style">
        <item name="android:layout_margin">8dip</item>
    </style>

    <style name="goodslist_gridView">
        <item name="android:padding">8dip</item>
        <item name="android:listSelector">#00000000</item>
        <item name="android:verticalSpacing">8dip</item>
        <item name="android:horizontalSpacing">8dip</item>
    </style>

    <style name="home_gridView">
        <item name="android:listSelector">#00000000</item>
        <item name="android:verticalSpacing">8dip</item>
        <item name="android:horizontalSpacing">8dip</item>
    </style>

    <style name="my_dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowBackground">@drawable/loading_bg</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/loading_bg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="DialogText">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="DialogText.Title">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="del_input_in_area">
        <item name="android:layout_width">30dip</item>
        <item name="android:layout_height">30dip</item>
        <item name="android:layout_gravity">center_vertical|center_horizontal</item>
        <item name="android:background">@drawable/input_delete</item>
    </style>

    <style name="go2_left_arrow">
        <item name="android:layout_width">30dip</item>
        <item name="android:layout_height">30dip</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_gravity">center_vertical|center_horizontal</item>
        <item name="android:background">@drawable/left_go2_arrow</item>
    </style>

    <style name="cart_header">
        <item name="android:layout_height">40dip</item>
        <item name="android:layout_marginTop">4dip</item>
    </style>

    <style name="mine_header_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">120dip</item>
    </style>

    <style name="input_style">
        <item name="android:background">@null</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingRight">8dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/goodsname</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="Theme.Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="Theme.Floating" parent="android:Theme.Translucent.NoTitleBar">
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="Theme.Overlay" parent="android:Theme">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Theme.AppTheme" parent="android:Theme">
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AnimationActivity" mce_bogus="1" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/anim_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/anim_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/back_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/back_exit</item>
    </style>

</resources>